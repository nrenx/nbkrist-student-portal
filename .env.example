# Supabase Configuration
# Replace these with your actual Supabase project credentials
# IMPORTANT: Never commit your actual credentials to version control!

# Required: Your Supabase project URL (e.g., https://your-project-id.supabase.co)
VITE_SUPABASE_URL=your_supabase_url_here

# Required: Your Supabase anon/public key (NOT the service role key for security reasons)
VITE_SUPABASE_KEY=your_supabase_anon_key_here

# Optional: Storage bucket name (defaults to 'student_data' if not specified)
VITE_SUPABASE_STORAGE_BUCKET=student_data

# Optional: Cache configuration
VITE_ENABLE_CACHE=true
VITE_CACHE_DURATION=300000

# Optional: Retry configuration
VITE_MAX_RETRIES=2
VITE_RETRY_DELAY=1000

# Optional: Visitor counter configuration
# Note: Requires the visitor_sessions table to be created in Supabase
VITE_ENABLE_VISITOR_COUNTER=true
VITE_VISITOR_HEARTBEAT_INTERVAL=30000
